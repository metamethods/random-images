{
  "name": "random-images",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-images": "ts-node-esm --transpile-only scripts/generate-images.ts",
    "test-images": "ts-node-esm --transpile-only scripts/test-images_toml.ts",
    "dev": "pnpm run test-images && pnpm run generate-images && next dev",
    "build": "pnpm run test-images && pnpm run generate-images && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "@types/node": "18.14.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "chalk": "^5.2.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "slugify": "^1.6.5",
    "toml": "^3.0.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7"
  }
}
